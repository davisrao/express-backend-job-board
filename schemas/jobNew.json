{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "company_handle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    },
    "salary": {
      "type": "integer",
      "minimum": 1
    },
    "equity": {
      "type": "number",
      "maximum": 1
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "company_handle"
  ]
}
